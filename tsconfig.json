{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "isolatedModules": true,
    "noEmit": true,
    "strict": true,
    "lib": ["es5", "dom"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": ["cypress", "node"]
  },
  "include": ["cypress/**/*.ts", "cypress.config.ts"]
}
